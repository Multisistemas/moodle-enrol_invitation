<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/gradebook/db" VERSION="20120828" COMMENT="XMLDB file for Moodle local/gradebook"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>  
    <TABLE NAME="ucla_grade_failed_updates" COMMENT="ucla_grade_failed_updates table retrofitted from MySQL">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="16" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="type" NEXT="foreignid"/>
        <FIELD NAME="foreignid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="courseid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="foreignid" NEXT="transactionid"/>
        <FIELD NAME="transactionid" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="attempts"/>
        <FIELD NAME="attempts" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" PREVIOUS="transactionid" NEXT="nextattempt"/>
        <FIELD NAME="nextattempt" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="attempts" NEXT="errortype"/>
        <FIELD NAME="errortype" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="nextattempt" NEXT="locked"/>
        <FIELD NAME="locked" TYPE="char" LENGTH="23" NOTNULL="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="errortype"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="unique" FIELDS="type, courseid, foreignid" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="attempts" UNIQUE="false" FIELDS="attempts" NEXT="nextattempt"/>
        <INDEX NAME="nextattempt" UNIQUE="false" FIELDS="nextattempt" PREVIOUS="attempts"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>