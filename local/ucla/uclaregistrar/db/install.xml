<?xml version="1.0" encoding="UTF-8" ?>
  <XMLDB PATH="admin/report/uclaregistrar/db" VERSION="20110513" COMMENT="XMLDB file for Moodle admin/report/uclaregistrar">
     <TABLES>
        <TABLE NAME="ucla_reg_classinfo" COMMENT="ucla_reg_classinfo table comes from Registrar and is filled on course creation in autocreate.php or in batch by misc/UCLA_fill_REGclassinfo_tbl.php." NEXT="ucla_reg_division">
          <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="subj_area"/>
            <FIELD NAME="subj_area" TYPE="char" LENGTH="7" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="coursenum"/>
            <FIELD NAME="coursenum" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="subj_area" NEXT="sectnum"/>
            <FIELD NAME="sectnum" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="coursenum" NEXT="crsidx"/>
            <FIELD NAME="crsidx" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="sectnum" NEXT="classidx"/>
            <FIELD NAME="classidx" TYPE="char" LENGTH="6" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="crsidx" NEXT="secidx"/>
            <FIELD NAME="secidx" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="classidx" NEXT="secttype"/>
            <FIELD NAME="secttype" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="secidx" NEXT="srs"/>
            <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="secttype" NEXT="term"/>
            <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="srs" NEXT="division"/>
            <FIELD NAME="division" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="term" NEXT="acttype"/>
            <FIELD NAME="acttype" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="division" NEXT="coursetitle"/>
            <FIELD NAME="coursetitle" TYPE="char" LENGTH="254" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="acttype" NEXT="sectiontitle"/>
            <FIELD NAME="sectiontitle" TYPE="char" LENGTH="240" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="coursetitle" NEXT="enrolstat"/>
            <FIELD NAME="enrolstat" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="sectiontitle" NEXT="session_group"/>
            <FIELD NAME="session_group" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="enrolstat" NEXT="session"/>
            <FIELD NAME="session" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="session_group" NEXT="url"/>
            <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="session" NEXT="crs_desc"/>
            <FIELD NAME="crs_desc" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="url"/>
          </FIELDS>
          <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="term"/>
            <KEY NAME="term" TYPE="unique" FIELDS="term, srs" PREVIOUS="primary" NEXT="subj_area"/>
            <KEY NAME="subj_area" TYPE="unique" FIELDS="term, subj_area, crsidx, secidx" PREVIOUS="term"/>
          </KEYS>
        </TABLE>
        <TABLE NAME="ucla_reg_division" COMMENT="ucla_reg_division table contains list of UCLA Divisions from Registrar, filled by hand first time, so we'll include the data in xmldb." PREVIOUS="ucla_reg_classinfo" NEXT="ucla_reg_subjectarea">
          <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="code"/>
            <FIELD NAME="code" TYPE="char" LENGTH="2" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="fullname"/>
            <FIELD NAME="fullname" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="code" NEXT="home"/>
            <FIELD NAME="home" TYPE="int" LENGTH="4" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="fullname" NEXT="timestamp"/>
            <FIELD NAME="timestamp" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="home"/>
          </FIELDS>
          <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="code"/>
            <KEY NAME="code" TYPE="unique" FIELDS="code" PREVIOUS="primary"/>
          </KEYS>
        </TABLE>
        <TABLE NAME="ucla_reg_subjectarea" COMMENT="ucla_reg_subjectarea table contains UCLA Subject Areas from Registrar for use in browseby scripts on front page. Filled by misc/UCLA_fill_REGsubjectarea_tbl.php which should be run once a quarter." PREVIOUS="ucla_reg_division" >
          <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="subjarea"/>
            <FIELD NAME="subjarea" TYPE="char" LENGTH="7" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="subj_area_full"/>
            <FIELD NAME="subj_area_full" TYPE="char" LENGTH="60" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="subjarea" NEXT="home"/>
            <FIELD NAME="home" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="subj_area_full" NEXT="modified"/>
            <FIELD NAME="modified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="home"/>
          </FIELDS>
          <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="subjarea"/>
            <KEY NAME="subjarea" TYPE="unique" FIELDS="subjarea" PREVIOUS="primary"/>
          </KEYS>
          <INDEXES>
            <INDEX NAME="subjarea_2" UNIQUE="false" FIELDS="subjarea, subj_area_full"/>
          </INDEXES>
        </TABLE>
    </TABLES>
  </XMLDB>
