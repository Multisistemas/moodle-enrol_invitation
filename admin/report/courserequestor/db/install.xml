<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/report/courserequestor/db" VERSION="20110721" COMMENT="tables for course requestor"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="ucla_request_classes" COMMENT="Table to request and track course builds" NEXT="ucla_request_crosslist">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="term"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="srs"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" PREVIOUS="term" NEXT="course"/>
        <FIELD NAME="course" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="srs" NEXT="department"/>
        <FIELD NAME="department" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="course" NEXT="instructor"/>
        <FIELD NAME="instructor" TYPE="char" LENGTH="200" NOTNULL="false" SEQUENCE="false" PREVIOUS="department" NEXT="contact"/>
        <FIELD NAME="contact" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" PREVIOUS="instructor" NEXT="crosslist"/>
        <FIELD NAME="crosslist" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="contact" NEXT="added_at"/>
        <FIELD NAME="added_at" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="crosslist" NEXT="action"/>
        <FIELD NAME="action" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" PREVIOUS="added_at" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" PREVIOUS="action" NEXT="mailinst"/>
        <FIELD NAME="mailinst" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" PREVIOUS="status" NEXT="hidden"/>
        <FIELD NAME="hidden" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="mailinst" NEXT="force_urlupdate"/>
        <FIELD NAME="force_urlupdate" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="hidden" NEXT="force_no_urlupdate"/>
        <FIELD NAME="force_no_urlupdate" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="force_urlupdate"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="ucla_request_crosslist" COMMENT="Table that holds the list of crosslisted classes (srs list)" PREVIOUS="ucla_request_classes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="term"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="srs"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" PREVIOUS="term" NEXT="aliassrs"/>
        <FIELD NAME="aliassrs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" PREVIOUS="srs" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" PREVIOUS="aliassrs"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>