<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/video_furnace/db" VERSION="20120524" COMMENT="XMLDB file for Moodle blocks/video_furnace"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="ucla_video_furnace" COMMENT="Contains the parsed video furnace data obtained from the datasource.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="term"/>
        <FIELD NAME="term" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="start_date"/>
        <FIELD NAME="start_date" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="term" NEXT="stop_date"/>
        <FIELD NAME="stop_date" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="start_date" NEXT="class"/>
        <FIELD NAME="class" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" COMMENT="Name of the class that the video belongs to." PREVIOUS="stop_date" NEXT="instructor"/>
        <FIELD NAME="instructor" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="class" NEXT="video_title"/>
        <FIELD NAME="video_title" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="instructor" NEXT="video_url"/>
        <FIELD NAME="video_url" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="video_title" NEXT="_del_flag"/>
        <FIELD NAME="_del_flag" TYPE="binary" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="Used during the db sync to determine whether or not the entry should be deleted." PREVIOUS="video_url"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>